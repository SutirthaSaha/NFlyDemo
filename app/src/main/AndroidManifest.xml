<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.nfly.dell.nflydemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.DashboardActivity" />
        <activity android:name=".activities.LearnActivity" />
        <activity android:name=".activities.PracticeActivity" />
        <activity android:name=".activities.KnowledgeBaseActivity" />
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.RegisterActivity" />
        <activity android:name=".activities.OnBoardRegisterActivity" />
        <activity android:name=".activities.CompanyWiseDetailsActivity" />
        <activity android:name=".activities.JobRoleWiseDetailsActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".activities.CourseStudyActivity" />
        <activity android:name=".activities.singleActivities.GDTopicsActivity" />
        <activity android:name=".activities.singleActivities.PracticePaperDetailsActivity" />
        <activity android:name=".activities.singleActivities.GDForAgainstActivity" />
        <activity android:name=".activities.singleActivities.TipsDetailsActivity" />
        <activity android:name=".activities.singleActivities.LearnCompanyPaperDetailsActivity" />
        <activity android:name=".activities.singleActivities.InterviewSubtopicsActivity" />
        <activity android:name=".activities.singleActivities.InterviewQuestionsActivity" />
        <activity android:name=".activities.FeedBackActivity" />
        <activity android:name=".activities.singleActivities.PracticeTestActivity"></activity>
    </application>

</manifest>